{
  "name": "sztyup/lauth",
  "description": "Auth plugin for Laravel 5",
  "license": "LGPL-3.0-only",
  "authors": [{
    "name": "Csupity László",
    "email": "sztyup@gmail.com"
  }],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "6.3.*",
    "illuminate/support": "^6.9",
    "illuminate/contracts": "^6.9",
    "laravel-doctrine/orm": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "~4.0",
    "mockery/mockery": "^1.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Sztyup\\LAuth": "src/LAuth"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sztyup\\Lauth\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sztyup\\Lauth\\LauthServiceProvider"
      ]
    }
  }
}
