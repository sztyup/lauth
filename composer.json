{
  "name": "sztyup/lauth",
  "description": "Auth plugin for Laravel 5",
  "license": "LGPL-3.0-only",
  "authors": [{
    "name": "Csupity László",
    "email": "sztyup@gmail.com"
  }],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "^5.7",
    "laravel/socialite": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "~3.0",
    "mockery/mockery": "^1.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Sztyup\\Lauth\\": "src/Lauth"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sztyup\\Lauth\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sztyup\\Lauth\\LauthServiceProvider"
      ]
    }
  }
}
