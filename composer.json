{
  "name": "sztyup/lauth",
  "description": "Auth plugin for Laravel 5",
  "license": "LGPL-3.0-only",
  "authors": [{
    "name": "Csupity László",
    "email": "sztyup@gmail.com"
  }],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3|^7.0",
    "illuminate/support": "^7.3|^8.0",
    "illuminate/contracts": "^7.3|^8.0",
    "laravel-doctrine/orm": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "~6.0",
    "mockery/mockery": "^1.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Sztyup\\LAuth\\": "src/LAuth"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sztyup\\LAuth\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sztyup\\LAuth\\LAuthServiceProvider"
      ]
    }
  }
}
